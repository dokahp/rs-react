{
  "name": "react-ts-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "ts-node server.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint --ext .js,.ts,.jsx,.tsx src",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --e2e",
    "cypress:report": "npx nyc report && npx nyc report --reporter=text-summary",
    "cypress:e2e": "npx start-server-and-test dev http-get://localhost:3001 cypress:run",
    "test:cypress": "npm run cypress:e2e && npm run cypress:report"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "@types/compression": "^1.7.2",
    "@types/react-notification-system": "^0.2.42",
    "@types/react-redux": "^7.1.25",
    "axios": "^1.3.5",
    "express": "^4.18.2",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "object-hash": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-notifications": "^1.7.4",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "react-svg": "^16.1.7",
    "react-toastify": "^9.1.2",
    "sort-by": "^1.2.0",
    "ts-node": "^10.9.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-svgr": "^2.4.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/cypress__code-coverage": "^3.10.0",
    "@types/express": "^4.17.17",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.3",
    "@vitest/coverage-istanbul": "^0.30.1",
    "babel-plugin-istanbul": "^6.1.1",
    "cypress": "^12.11.0",
    "eslint": "^8.35.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^21.1.0",
    "nyc": "^15.1.0",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-plugin-istanbul": "^4.0.1",
    "vitest": "^0.30.1"
  },
  "license": "MIT"
}
